          <argument>
            <kind>1</kind>
            <string>/// Landing
if (!onGround)
{
    zspeed-=gravity_force;
    z += zspeed

  if (z &lt;= radius)
  {
    if (z &gt;= 8 and !noTrack)
    {
      z = radius;
      zspeed = 0;
      onGround = true;
      hit=false;
      bump_radius = 2;
      jumping = false;
      jump_release=false;
      jump_force =4;
      
      if closing &gt; 0 {
      if objGameData.character_id[0] == 1 {
        sprite_index=sprSonicSSEnd;
        }
      else if objGameData.character_id[0] == 2 {
        sprite_index=sprTailsSSEnd;
      } else sprite_index=sprKnucklesSSEnd;
      
        image_speed = .25;
      }
      
        if kill &gt; 0 &amp;&amp; alarm[0] == -1 {
    
            alarm[0]=30;
    
        }
    }
    else {
    
    if (z &lt;= -1500)
    {
    
        if kill &lt; 1 {
            objSSMessages.alarm[1] = 30;
            kill = 2;
        }
        if alarm[0] = -1 {
            
            alarm[0]=30;
            exit;
      }
    }
    }
  }

  zspeed -= 0.2;
  zspeed = max(zspeed, -8);
}
